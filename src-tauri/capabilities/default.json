{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "http:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        {
          "url": "https://api.opendaw.studio/**"
        },
        {
          "url": "https://logs.opendaw.studio/**"
        }
      ]
    }
  ]
}
